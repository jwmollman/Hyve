@model Hyve.ViewModels.Home.PostListViewModel

@using Hyve.Models;

@{ 
    ViewBag.Title = "Home";
}

<div class="listing">
    @foreach (Post post in Model.Posts) {
        <div class="item">
            <div class="title">
                @Html.ActionLink(post.Title, "Post", "Home", new { id = post.Id }, null)
            </div>
            <div class="info">(sub information)</div>
        </div>
    }
</div>
